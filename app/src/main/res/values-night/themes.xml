    <style name="settheme" parent="Theme.MaterialComponents.Light.NoActionBar">
        <item name="colorPrimary">@color/primary</item>
        <item name="colorPrimaryDark">#F7F3EB</item>
        <item name="android:navigationBarColor">#F7F3EB</item>
        <item name="colorAccent">@color/accent</item>
    </style>

</resources>